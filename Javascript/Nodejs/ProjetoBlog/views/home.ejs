<% posts.forEach(function(post){ %>
    <h1><%= post.title %></h1>
    <%if(post.postcontent.length > 100){%>
        <p><%= post.postcontent.substring(0, 100)%>... <a>Read More</a></p>
    <% }else {%>
        <p><%= post.postcontent%> <a>Read More</a></p>
    <%}%>
    <br>
<%}) %> 


